      'insert into PESSOAS'
      
        '  (CODIGO, TIPO, CNPJCPF, RAZAOSOCIAL, FANTASIA, ENDERECO, BAIRR' +
        'O, CEP, '
      
        '   CODCIDADE, UF, PROXIMIDADE, FONE1, FONE2, FAX, EMAIL, DATACAD' +
        'ASTRO, '
      
        '   ATIVO, CODEMPRESA, INSCIDENT, AGENCIA, BAIRROCOMERCIAL, BAIRR' +
        'OCOMERCIALCONJUGE, '
      
        '   BLOQUEADO, CELPROPRIETARIO1, CELPROPRIETARIO2, CEPCOMERCIAL, ' +
        'CEPCOMERCIALCONJUGE, '
      
        '   COBRANCA, CODAGENDA1, CODAGENDA2, CODBANCO, CODCIDADECOMERCIA' +
        'L, CODCIDADECOMERCIALCONJUGE, '
      
        '   CODCIDADENASC, CODCONDICAOPAGTO, COMPLEMENTOCOMERCIAL, COMPLE' +
        'MENTOCOMERCIALCONJUGE, '
      
        '   CONDICAOPAGTO_DIA_PROXIMO_MES, CONTA, CONTATO, CONTRATO, CORR' +
        'ESBAIRRO, '
      
        '   CORRESCEP, CORRESCODCIDADE, CORRESCOMPLEMENTO, CORRESENDERECO' +
        ', CORRESUF, '
      
        '   CRT, DATA_ADMISSAO, DATA_CONTRATO1, DATA_CONTRATO2, DATA_FGTS' +
        ', DATANASCIMENTO, '
      
        '   DEMISSAO_DT, DEMISSAO_MOTIVO, DEMISSAO_OBSERVACAO, DESCONTO_P' +
        'RODUTO, '
      
        '   DIAS, EMPRESA_TRABALHO, ENDERECOCOMERCIAL, ENDERECOCOMERCIALC' +
        'ONJUGE, '
      
        '   ESTADO_CIVIL, FLCASAPROPRIA, FONE_TRABALHO, GRAU_INSTRUCAO, H' +
        'ABILITACAO_CATEGORIA, '
      
        '   HABILITACAO_CODCIDADE, HABILITACAO_N, HABILITACAO_VALIDADE, I' +
        'NSCIDENT_DATA, '
      
        '   INSCIDENT_ORGAO, INSCIDENT_UF, LIMITE_DESCONTO, LIMITECREDITO' +
        ', LOJA01, '
      
        '   LOJA02, LOJA03, MAE, PAI, MOTIVO, NACIONALIDADE, NASCIMENTO_C' +
