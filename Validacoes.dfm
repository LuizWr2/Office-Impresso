    Transaction = TransaConfiguracao
    SQL.Strings = (
      'select CC.*, CAR.obrigatorio as OBRIGATORIOCAR'
      'from CONFIGURACAO_COMPONENTE CC'
      
        'LEFT JOIN CONFIGURACAO_FORM CF ON CF.CODIGO=CC.CODCONFIGURACAO_F' +
        'ORM'
      
        'LEFT JOIN CONFIGURACAO_ACOES_REGRA CAR on CAR.CODCONFIGURACAO_CO' +
        'MPONENTE = CC.CODIGO'
      'where (CF.DESCRICAO = :Form)'
      ''
